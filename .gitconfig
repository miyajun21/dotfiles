[user]
	name = miyajun21
	email = miyajun21@gmail.com
[push]
  default = current
[alias]
  gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset% cd %Cblue%cn %Cred%d %Creset%s'
  st = status
[color]
    ui = true
[core]
    editor = vim
    pager = diff-highlight | less
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
